{
  "name": "maps-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "babel-preset-expo": "~54.0.0",
    "expo": "~54.0.22",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-dotenv": "^3.4.11",
    "react-native-maps": "1.20.1",
    "react-native-safe-area-context": "~5.6.0"
  },
  "private": true,
  "android": {
    "config": {
      "googleMaps": {
        "apiKey": "${GOOGLE_MAPS_API_KEY}"
      }
    }
  },
  "ios": {
    "config": {
      "googleMapsApiKey": "${GOOGLE_MAPS_API_KEY}"
    }
  },
  "plugins": [
    [
      "react-native-dotenv",
      {
        "moduleName": "@env",
        "path": ".env"
      }
    ]
  ]
}
